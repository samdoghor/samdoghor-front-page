{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.samdoghor.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "samdoghor.com"
        }
      ]
    }
  ]
}
